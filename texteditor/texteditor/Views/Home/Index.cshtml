@model List<texteditor.Models.Blog>
@{
    ViewBag.Title = "Index";
}

<h2>Text Editör Eğitimi</h2>
<table class="table table-hover table-striped table-bordered">
    <tr>
        <th style="width:100px">
            <b>Ad Soyad</b>
        </th>
        <th>
            <b>İcerik</b>
        </th>

    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @item.AdSoyad
            </td>
            <td>
                @item.icerik
            </td>


        </tr>
    }



</table>
